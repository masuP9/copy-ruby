{
  "manifest_version": 2,
  "name": "Copy Ruby",
  "version": "0.0.1",
  "description": "copy text formatter in ruby element",
  "author": "masuP9",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["app.js"]
    }
  ],
  "permissions": ["storage"],
  "options_ui": {
    "page": "options/option.html",
    "chrome_style": true
  }
}
